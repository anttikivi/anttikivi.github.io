{
    "extends": "astro/tsconfigs/strictest",
    "include": [".astro/types.d.ts", "**/*", "src/pages/.well-known/**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "plugins": [
            {
                "name": "@astrojs/ts-plugin"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
